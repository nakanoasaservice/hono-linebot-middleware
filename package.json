{
	"name": "@nakanoaas/hono-linebot-middleware",
	"version": "0.0.1",
	"description": "Hono middleware for Line Bot built on Web Standards",
	"author": "Nakano as a Service",
	"license": "Apache-2.0",
	"type": "module",
	"sideEffects": false,
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "tsdown",
		"cf-typegen": "wrangler types",
		"check-types": "tsc --noEmit",
		"check:fix": "biome check --write",
		"check": "biome check",
		"test": "vitest"
	},
	"peerDependencies": {
		"hono": "^4.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"@cloudflare/vitest-pool-workers": "^0.10.10",
		"tsdown": "^0.16.7",
		"typescript": "^5.9.3",
		"vitest": "~3.2.4",
		"wrangler": "^4.50.0"
	},
	"packageManager": "pnpm@10.20.0"
}
