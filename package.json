{
	"name": "@nakanoaas/hono-linebot-middleware",
	"version": "0.0.5",
	"description": "Hono middleware for LINE Bot webhook signature validation built on Web Standards",
	"author": "Nakano as a Service",
	"license": "Apache-2.0",
	"type": "module",
	"sideEffects": false,
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"homepage": "https://github.com/nakanoasaservice/hono-linebot-middleware",
	"repository": {
		"type": "git",
		"url": "https://github.com/nakanoasaservice/hono-linebot-middleware.git"
	},
	"bugs": {
		"url": "https://github.com/nakanoasaservice/hono-linebot-middleware/issues"
	},
	"keywords": [
		"hono",
		"line",
		"middleware",
		"cloudflare",
		"workers"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"cf-typegen": "wrangler types",
		"check-types": "tsc --noEmit",
		"check:fix": "biome check --write",
		"check": "biome check",
		"test": "vitest"
	},
	"peerDependencies": {
		"hono": "^4.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"@cloudflare/vitest-pool-workers": "^0.10.10",
		"@types/node": "^24.10.1",
		"tsdown": "^0.17.1",
		"typescript": "^5.9.3",
		"vitest": "~3.2.4",
		"wrangler": "^4.50.0"
	},
	"packageManager": "pnpm@10.23.0"
}
